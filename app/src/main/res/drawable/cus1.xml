<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#317773" /> <!-- Màu nền của button -->
            <corners android:radius="10dp" /> <!-- Bo góc button -->
            <stroke
                android:width="2dp"
                android:color="@color/black" /> <!-- Độ dày viền của button -->
        </shape>
    </item>
</layer-list>